import java.util.Random;

public class PetArt {

    // Method to print new Labrador Dog art
    public static void printLabradorDog() {
        System.out.println("    ___");
        System.out.println(" __/_  `.  .-\"\"\"-.");
        System.out.println(" \\_,` | \\-'  /   )`-')");
        System.out.println("  \"\") `\"`    \\  ((`\"`");
        System.out.println(" ___Y  ,    .'7 /|");
        System.out.println(" (_,___/...-` (_/_/");
    }

    // Method to print new Bulldog Dog art
    public static void printBulldogDog() {
        System.out.println("  (\\,--------'()'--o");
        System.out.println("   (_    ___    /~\"");
        System.out.println("    (_)_)  (_)_)");
    }

    // Method to print the new Poodle Dog art you provided
    public static void printPoodleDog() {
        System.out.println("  __      _");
        System.out.println(" o'')}____//");
        System.out.println("  `_/      )");
        System.out.println("  (_(_/-(_/");
    }

    // Method to print new Beagle Dog art
    public static void printBeagleDog() {
        System.out.println("   __");
        System.out.println(" o-''|\\_____/) ");
        System.out.println(" \\_/|_)     ) ");
        System.out.println("    \\  __  / ");
        System.out.println("    (_/ (_/    ");
    }

    // Method to print new German Shepherd Dog art
    public static void printGermanShepherdDog() {
        System.out.println("     |\\_/|                  ");
        System.out.println("     | @ @   Woof! ");
        System.out.println("     |   <>              _  ");
        System.out.println("     |  _/\\------____ ((| |))");
        System.out.println("     |               `--' |   ");
        System.out.println(" ____|_       ___|   |__.' ");
        System.out.println("/_/_____/____/_______|");
    }

    // Method to print the custom dog art you provided
    public static void printCustomDogArt() {
        System.out.println("");
        System.out.println("~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~");
        System.out.println("");
        System.out.println("            /~~~~~~~~\\                           _");
        System.out.println("    ##\\__/ @)      ~~~~~~~~\\                     \\ \\ ) )");
        System.out.println("    |              /~~\\~~~~~                ((    |  \\");
        System.out.println("     \\    /           |                          /   |");
        System.out.println("      (~~~   /         \\____________/~~~~~~~~~~~~   /");
        System.out.println("       ~~~~|~                                     /");
        System.out.println("           :                                      |");
        System.out.println("            \\                                     |");
        System.out.println("            |                               /      \\");
        System.out.println("             \\  \\_         :         \\     /~~~\\    |");
        System.out.println("             /   :~~~~~|   :~~~~~~~~~~|   :     :   :");
        System.out.println("            /    :    /    :         /    :    /    :");
        System.out.println("        (~~~     )(~~~     )     (~~~     )(~~~     )");
        System.out.println("         ~~~~~~~~  ~~~~~~~~       ~~~~~~~~  ~~~~~~~~");
    }

    // Method to print the Mountain Lion Cat art (CatOne)
    public static void printCatOne() {
        System.out.println("(`-''-/`).___..--''`-._ ");
        System.out.println(" `6_ 6  )   `-.  (     ).`-.__.`) ");
        System.out.println(" (_Y_.)'  ._   )  `._ `. ``-..-' ");
        System.out.println("   _..`--'_..-_/  /--'_.'");
        System.out.println("  ((((.-''  ((((.'  (((.-' ");
    }

    // Method to pick a random dog or cat art and print it
    public static void printRandomDogOrCatArt() {
        Random rand = new Random();
        int randomIndex = rand.nextInt(7); // Random number between 0 and 6

        switch (randomIndex) {
            case 0:
                printLabradorDog(); // Print Labrador art
                break;
            case 1:
                printBulldogDog(); // Print Bulldog art
                break;
            case 2:
                printPoodleDog(); // Print Poodle art
                break;
            case 3:
                printBeagleDog(); // Print Beagle art
                break;
            case 4:
                printGermanShepherdDog(); // Print German Shepherd art
                break;
            case 5:
                printCustomDogArt(); // Print Custom Dog art
                break;
            case 6:
                printCatOne(); // Print Mountain Lion Cat art
                break;
        }
    }
}


